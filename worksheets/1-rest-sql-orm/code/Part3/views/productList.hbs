<h1>Product List</h1>
<table style="width: 1000px">
<tr>
    <th>Title</th>
    <th>Price</th>
    <th>Created At</th>
    <th>Deleted At</th>
    <th>Tags</th>
</tr>
{{#each root}}
    <tr>
        <td>{{this.title}}</td>
        <td>{{this.price}}</td>
        <td>{{this.created_at}}</td>
        <td>{{this.deleted_at}}</td>
        <td>{{this.tags}}</td>
    <tr/>
{{/each}}
</table>
