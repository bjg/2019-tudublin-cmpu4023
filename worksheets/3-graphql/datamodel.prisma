type Product {
  id: ID! @unique
  category: Category!
  title: String!
  actor: String!
  price: Float!
  special: Int
  common_prod_id: Int! @unique
}

type Category {
  id: ID! @unique
  categoryname: String! @unique
}

type Inventory {
  id: ID! @unique
  prod_id: Product!
  quan_in_stock: Int!
  sales: Int!
}

type Reorder {
  id: ID! @unique
  prod_id: Product!
  date_low: DateTime!
  quan_low: Int!
  date_reordered: DateTime
  quan_reordered: Int
  date_expected: DateTime
}
