type Reorder {
  id: ID! @unique
  prod_id: [Products!]!
  date_low: DateTime
  quan_low: Int
  date_reordered: DateTime
  quan_reordered: Int
  date_expected: DateTime
}

type Inventory {
  id: ID! @unique
  prod_id: [Products!]!
  quan_in_stock: Int!
  sales: Int
}

type Products{
  id: ID! @unique
  categorey: [Categorey!]!
  title: String!
  actor: String
  price: Float!
  special: Int
  common_prod_id: Int
}

type Categorey{
  id: ID! @unique
  categorey_name: String!
}