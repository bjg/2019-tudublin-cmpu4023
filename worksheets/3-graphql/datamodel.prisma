type Order {
    id: ID! @unique
    amount: Float!
    orderlines: [Orderline!]!
}

type Product {
    id: ID! @unique
    title: String!
    price: Float!
    category: Category!
    orderlines: [Orderline!]!
}

type Category {
    id: ID! @unique
    name: String! @unique
    products: [Product!]!
}

type Orderline {
    id: ID! @unique
    quantity: Int!
    product: Product!
    order: Order!
}

