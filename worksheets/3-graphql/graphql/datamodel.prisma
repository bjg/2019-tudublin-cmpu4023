type Category {
  category: ID! @unique
  categoryname: String!
}

type Product {
  prod_id: ID! @unique
  category: Category
  title: String!
  actor: String
  price: Float!
  special: Float
  common_prod_id: Int
}

type Inventory {
  id: ID! @unique
  product: Product
  quan_in_stock: Int
  sales: Int
}

type Reorder {
  id: ID! @unique
  prod_id: Product
  date_low: DateTime
  quan_low: Int
  date_reordered: DateTime
  quan_reordered: Int
  date_expected: DateTime
}