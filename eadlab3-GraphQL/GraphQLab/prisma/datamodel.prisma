type Products {
  prod_id: ID! @unique @default(value: "1")
  category: [Categories!]!
  title: String! @default(value: "Title")
  actor: String!
  price: Int!
  special: Int!
  common_prod_id: Int
}

type Reorder {
    prod_id: [Products!]!
    date_low: DateTime!
    quan_low: Int!
    date_reordered: DateTime!
    quan_reordered: Int!
    date_expected: DateTime!
}

type Inventory {
    prod_id: [Products!]!
    quan_in_stock: Int!
    sales: Int!
}
type Categories {
  category: ID! 
  categoryname: String!
}